@article{poupeau_ledition_2004,
	title = {L'édition électronique change tout et rien. {Dépasser} les promesses de l'édition électronique},
	abstract = {Les problèmes de l'édition électronique diffèrent en fonction des types d'information dont dispose l'historien : sources mises à disposition par les institutions de conservation, éditions érudites, outils de recherche comme les dictionnaires ou travaux historiques publiés dans des articles ou des monographies. La lecture de l'information se réalise différemment en fonction de ce qui est recherché : linéaire, zapping ou mode recherche. Comment proposer un outil de lecture qui « facilite » la lecture ? (présentation, typographie, graphisme). Le référencement lui dépend des métadonnées : les standards en sont encore à leur mise en place : Dublin Core ou OAI. On est aussi à l'orée de la syndication de contenu (mise en commun de sites Web). Pour ce qui est de la conservation des données, des interfaces ou des supports : affaire à suivre. Il s'agit aussi de conserver l'accessibilité à tous de toutes les données du Web qui ont tendance à être « récupérées »},
	journal = {Le Médiéviste et l'Ordinateur},
	author = {Poupeau, Gautier},
	year = {2004},
}

