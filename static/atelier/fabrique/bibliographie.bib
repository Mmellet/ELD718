
@inproceedings{rebillard_information_2006,
	title = {L'information journalistique sur l'internet, entre diffusion mass-médiatique et circulation réticulaire de l'actualité},
	abstract = {Depuis plus de dix ans maintenant, l'Internet s'est progressivement ouvert au « grand public » et a pu constituer, du même coup, un nouvel espace pour le journalisme. Dès le départ, certains ont envisagé un renversement complet du système médiatique établi, renouvelé sur les bases d'une production atomisée et non-marchande. Ces derniers temps encore, une telle perspective a pu être ravivée à la faveur de l'apparition des blogs, prenant en cela la suite des webzines ou des sites personnels quelques années plus tôt. Au-delà de ces visions bien souvent mâtinées d'idéologie, la présente recherche vise à dresser un bilan du développement effectif de l'information journalistique sur l'Internet. Sans occulter les potentialités offertes par ce nouveau dispositif socio-technique, mais sans non plus tirer un trait sur les logiques structurant l'évolution du journalisme sur la longue durée. Dans ce cadre, un état des différents lieux de diffusion montre une variété assez forte : si certaines formules se rapprochent des modèles mass-médiatiques traditionnels, d'autres intitiatives s'appuient à l'inverse sur les opportunités offertes par l'Internet pour proposer de nouvellles impulsions au journalisme. Mais, davantage que cette diversité, le fait le plus remarquable réside sans doute dans les échanges de liens et de contenus journalistiques entre ces différents lieux de diffusion. La circulation des informations, au sein de réseaux reliant les différents espaces de diffusion, semble constituer une modalité essentielle du développement du journalisme sur l'Internet.},
	language = {fr},
	urldate = {2022-09-11},
	booktitle = {Doc {Soc} 06},
	publisher = {ADBS Editions},
	author = {Rebillard, Franck},
	year = {2006},
	pages = {213--226},
}

